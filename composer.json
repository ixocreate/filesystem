{
    "name": "kiwi-suite/filesystem",
    "description": "kiwi-suite/filesystem",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Thomas Payer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1",
        "league/flysystem": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0.6",
        "friendsofphp/php-cs-fixer": "^2.5",
        "kiwi-suite/application": "dev-develop",
        "kiwi-suite/servicemanager": "dev-develop",
        "php-coveralls/php-coveralls": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "KiwiSuite\\Filesystem\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KiwiSuiteMisc\\Filesystem\\": "tests/misc/",
            "KiwiSuiteTest\\Filesystem\\": "tests/src/"
        }
    }
}

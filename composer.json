{
    "name": "ixocreate/filesystem-package",
    "description": "IXOCREATE Filesystem Package",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "IXOCREATE",
            "email": "opensource@ixocreate.com",
            "homepage": "https://www.ixocreate.com",
            "role": "Developer"
        }
    ],
  "minimum-stability": "dev",
  "prefer-stable": true,
    "require": {
        "php": "^7.2 || ^8.0",
        "ixocreate/application": "dev-develop",
        "league/flysystem": "^2.0"
    },
    "require-dev": {
        "ixocreate/coding-standard": "^2.1",
        "ixocreate/test": "^0.1",
        "league/flysystem-aws-s3-v3": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Ixocreate\\Filesystem\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ixocreate\\Misc\\Filesystem\\": "tests/misc/",
            "Ixocreate\\Test\\Filesystem\\": "tests/src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "fix-cs": "vendor/friendsofphp/php-cs-fixer/php-cs-fixer fix --config vendor/ixocreate/coding-standard/.php_cs"
    }
}
